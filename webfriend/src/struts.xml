<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="struts2" extends="struts-default">
		<action name="register" class="com.nf.action.RegisterAction">
			<result name="success"  type="redirect">/registersuccess.jsp</result>
			<result name="input"  type="redirect">/user/register.jsp</result>
		</action>
		<action name="login" class="com.nf.action.LoginAction">
			<result name="success"  type="redirect">/loginsuccess.jsp</result>
			<result name="input"  type="redirect">/user/login.jsp</result>
		</action>
		<action name="logout" class="com.nf.action.LogOutAction">
			<result name="success"  type="redirect">/user/login.jsp</result>
		</action>
	</package>
</struts>	